{
    "name": "cwp/agency-extensions",
    "description": "Base module for the default CWP theme to add features",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cwp"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "cwp/cwp": "^2",
        "silverstripe/framework": "^4.1",
        "symbiote/silverstripe-gridfieldextensions": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "suggest": {
        "heyday/silverstripe-colorpalette": "Provide a nicer UI for the color scheme picker"
    },
    "autoload": {
        "psr-4": {
            "CWP\\AgencyExtensions\\": "src/",
            "CWP\\AgencyExtensions\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "thirdparty/TinyMCE-FontAwesome-Plugin/fontawesome"
        ],
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
